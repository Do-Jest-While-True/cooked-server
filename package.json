{
	"main": "main.js",
	"scripts": {
		"start": "node main.js",
		"start-dev": "nodemon main.js",
		"seed": "node seed.js",
		"lint": "eslint ./ --ignore-path .gitignore",
		"lint-fix": "npm run lint -- --fix",
		"prettify": "prettier --write \"**/*.{js,jsx,json,css,scss,md}\""
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged"
		}
	},
	"lint-staged": {
		"*.{js,jsx}": [
			"prettier --write",
			"npm run lint-fix",
			"git add"
		],
		"*.{css,scss,json,md}": [
			"prettier --write"
		]
	},
	"dependencies": {
		"axios": "^0.20.0",
		"express": "^4.17.1",
		"faker": "^5.1.0",
		"pg": "^8.4.1",
		"pg-hstore": "^2.3.3",
		"sequelize": "^6.3.5"
	},
	"devDependencies": {
		"babel-eslint": "^10.1.0",
		"chalk": "^4.1.0",
		"eslint": "^7.11.0",
		"eslint-config-prettier": "^6.12.0",
		"husky": "^4.3.0",
		"lint-staged": "^10.4.0",
		"morgan": "^1.10.0",
		"nodemon": "^2.0.4",
		"prettier": "^2.1.2"
	},
	"private": true
}
